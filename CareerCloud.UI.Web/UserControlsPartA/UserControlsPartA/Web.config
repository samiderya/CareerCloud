<configuration>

  <appSettings>
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None" />
  </appSettings>
  
  <system.web>
    <compilation debug="true" targetFramework="4.5.2" />
    <httpRuntime targetFramework="4.5.2" />

    <pages>
      <controls>
        <add tagPrefix="user" tagName="RandomImage"
             src="~/UserControls/RandomImage.ascx"/>

        <add tagPrefix="user" tagName="RandomImageWithProperty"
             src="~/UserControls/RandomImageWithProperty.ascx"/>

        <add tagPrefix="user" tagName="TabScrip"
             src="~/UserControls/TabScrip.ascx"/>

        <add tagPrefix="user" tagName="AddressForm"
             src="~/UserControls/AddressForm.ascx"/>

      </controls>
    </pages>
    
  </system.web>



</configuration>
